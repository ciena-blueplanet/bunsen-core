    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "eslint": "^3.2.1",
    "eslint-config-frost-standard": "^6.0.0",
    "eslint-plugin-mocha": "^4.3.0",
